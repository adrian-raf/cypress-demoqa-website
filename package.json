{
  "scripts": {
    "cy:open": "cypress open",
    "test:ui": "cypress run --headed --no-exit --spec",
    "test:headless": "cypress run --headless --spec",
    "test:positive": "cypress run --expose grepTags=@positive,grepFilterSpecs=true",
    "test:negative": "cypress run --expose grepTags=@negative,grepFilterSpecs=true",
    "test:all": "cypress run"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.3.1",
    "cypress": "^15.10.0"
  },
  "dependencies": {
    "@cypress/grep": "^6.0.0"
  }
}